<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_fetch_attendance_wizard_form" model="ir.ui.view">
            <field name="name">fetch.attendance.wizard.form</field>
            <field name="model">fetch.attendance.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="start_date"/>
                            </group>
                            <group>
                                <field name="end_date"/>
                            </group>
                        </group>
                    </sheet>
                    <footer>
                        <button name="fetch_attendance_data" string="Confirm" type="object" class="oe_highlight"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>