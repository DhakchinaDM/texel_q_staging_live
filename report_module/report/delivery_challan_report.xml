        <record id="report_delivery_challan_record_1" model="ir.actions.report">
            <field name="name">Delivery Challan</field>
            <field name="model">stock.picking</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">report_module.report_delivery_challan_report_template_new</field>
            <field name="report_file">report_module.report_delivery_challan_report_template_new</field>
            <field name="paperformat_id" ref="report_module.paperformat_delivery_challan_report"/>
            <field name="print_report_name">'Delivery Challan'</field>
            <field name="binding_model_id" ref="stock.model_stock_picking"/>
            <field name="binding_type">report</field>
        </record>

        <template id="report_delivery_challan_report_template_new">
            <t t-call="web.html_container">
                <t t-call="web.basic_layout">
                    <t t-foreach="docs" t-as="o">
                        <div class="page">
                            <table class="table table-sm"
                                   style="width: 95%; margin: 2px; margin-left: 25px; margin-right: 15px; border: 1px solid black; font-size: 17px; border-spacing: 1em 0.5em; padding: 0 2em 1em 0; border-radius: 2px;">
                                <tr>
                                    <td colspan="2">
                                        <img width="100%"
                                             t-attf-src="/web/binary/company_logo"
                                             alt="Logo"/>
                                    </td>
                                    <td colspan="10" align="center">
                                        <div style="font-size: 20px">
                                            <span t-field="o.user_id.company_id.name"/>
                                            <br/>
                                            <t t-if="o.user_id.company_id.street">
                                                <span t-field="o.user_id.company_id.street"/>
                                            </t>
                                            <t t-if="o.user_id.company_id.street2">
                                                <span t-field="o.user_id.company_id.street2"/>,
                                            </t>
                                            <t t-if="o.user_id.company_id.city">
                                                <span t-field="o.user_id.company_id.city"/>,
                                            </t>
                                            <t t-if="o.user_id.company_id.state_id.name">
                                                <span t-field="o.user_id.company_id.state_id.name"/>
                                            </t>
                                            <t t-if="o.user_id.company_id.zip">
                                                -
                                                <span t-field="o.user_id.company_id.zip"/>
                                            </t>
                                        </div>
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="12" align="center">
                                        <strong>Delivery Challan</strong>
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="3">
                                        <strong>
                                            GST
                                        </strong>
                                    </td>
                                    <td colspan="3">
                                        <span t-field="o.company_id.vat"/>
                                    </td>
                                    <td colspan="3">
                                        <strong>
                                            D.C
                                        </strong>
                                    </td>
                                    <td colspan="3">
                                        <span t-field="o.name"/>
                                    </td>
                                </tr>
                                <tr>
                                    <td width="20%" colspan="3">
                                        <strong>
                                            Phone
                                        </strong>
                                    </td>
                                    <td colspan="3">
                                        <span t-field="o.user_id.company_id.phone"/>
                                    </td>
                                    <td colspan="3">
                                        <strong>
                                            Date
                                        </strong>
                                    </td>
                                    <td colspan="3">
                                        <span t-field="o.scheduled_date" t-options='{"widget": "date"}'/>
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="6"/>
                                    <td colspan="3">
                                        <strong>Transport Name</strong>
                                    </td>
                                    <td colspan="3" class="padding_all tleft tbottom ttop text-center">
                                        <span>-</span>
                                        <!--                                        <span t-field="o.origin"/>-->
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="12" align="center">
                                        <strong>TO ADDRESS :</strong>
                                        <small>
                                            <t t-set="address">
                                                <address class="mb-0" t-field="o.partner_id"
                                                         t-options='{"widget": "contact", "fields": ["address", "name"], "no_marker": True}'/>
                                            </t>
                                            <div>
                                                <t t-esc="address"/>
                                            </div>
                                        </small>
                                    </td>
                                </tr>


                                <tr align="center">
                                    <td colspan="3">
                                        <strong>Mode of Transport</strong>
                                    </td>
                                    <td colspan="3">
                                        <strong>Contact Person Name &amp; Phone No</strong>
                                    </td>
                                    <td colspan="3">
                                        <strong>Supplier Reference</strong>
                                    </td>
                                    <td colspan="3">
                                        <strong>Place of Supply</strong>
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="3" class="padding_all tleft tbottom ttop text-center">
                                        <strong align="center">-</strong>
                                    </td>
                                    <td colspan="3">
                                        <span t-foreach="o.partner_id.child_ids.filtered(lambda r: r.type == 'contact')"
                                              t-as="contact">
                                            <strong style="font-size:12px;" t-field="contact.name"/>
                                        </span>
                                    </td>
                                    <td colspan="3">
                                        <strong t-field="o.bill_ref"></strong>
                                    </td>
                                    <td colspan="3" class="padding_all tleft tbottom ttop text-center">
                                        <strong align="center">-</strong>
                                    </td>
                                </tr>
                                <tr align="center">
                                    <th width="10%" colspan="1"
                                        style="font-size:17px;border-right:1px solid #143980;color:black;background-color:#c6cacf; border:1px solid #000;border-color: black;">
                                        <strong>SL.NO</strong>
                                    </th>
                                    <th width="10%" colspan="1"
                                        style="font-size:17px;border-right:1px solid #143980;color:black;background-color:#c6cacf; border:1px solid #000;border-color: black;">
                                        <strong>PART NO</strong>
                                    </th>
                                    <th width="20%" colspan="2"
                                        style="font-size:17px;border-right:1px solid #143980;color:black;background-color:#c6cacf; border:1px solid #000;border-color: black;">
                                        <strong>PART DESCRIPTION</strong>
                                    </th>
                                    <th width="15%" colspan="2"
                                        style="font-size:17px;border-right:1px solid #143980;color:black;background-color:#c6cacf; border:1px solid #000;border-color: black;">
                                        <strong>SERVICE</strong>
                                    </th>
                                    <th width="10%" colspan="1"
                                        style="font-size:17px;border-right:1px solid #143980;color:black;background-color:#c6cacf; border:1px solid #000;border-color: black;">
                                        <strong>UOM</strong>
                                    </th>
                                    <th width="10%" colspan="1"
                                        style="font-size:17px;border-right:1px solid #143980;color:black;background-color:#c6cacf; border:1px solid #000;border-color: black;">
                                        <strong>QTY</strong>
                                    </th>
                                    <th width="10%" colspan="2"
                                        style="font-size:17px;border-right:1px solid #143980;color:black;background-color:#c6cacf; border:1px solid #000;border-color: black;">
                                        <strong>UNIT PRICE</strong>
                                    </th>
                                    <th width="15%" colspan="2"
                                        style="font-size:17px;border-right:1px solid #143980;color:black;background-color:#c6cacf; border:1px solid #000;border-color: black;">
                                        <strong>TOTAL VALUE</strong>
                                    </th>
                                </tr>
                                <t t-set="qty" t-value="0"/>
                                <t t-set="unit_price" t-value="0"/>
                                <t t-set="total_value" t-value="0"/>
                                <t t-foreach="o.move_ids_without_package" t-as="line">
                                    <t t-set="qty" t-value="qty + line.quantity"/>
                                    <t t-set="unit_price" t-value="unit_price + line.product_id.list_price"/>
                                    <t t-set="total_value" t-value="total_value + line.product_id.list_price"/>
                                    <tr>
                                        <td align="center" colspan="1">
                                            <span t-esc="line_index+1"/>
                                        </td>
                                        <td colspan="1">
                                            <span t-esc="line.product_id.default_code"/>
                                        </td>
                                        <td colspan="2">
                                            <span t-esc="line.product_id.name"/>
                                        </td>
                                        <td colspan="2">
                                            <span t-esc="line.product_id.categ_id.name"/>
                                        </td>
                                        <td align="center" colspan="1">
                                            <!--                                            <span t-esc="line.product_id.categ_id.name"/>-->
                                            <span>NO'S</span>
                                        </td>
                                        <td align="right" colspan="1">
                                            <span t-esc="line.quantity"/>
                                        </td>
                                        <td align="right" colspan="2">
                                            <span t-esc="line.product_id.list_price"/>
                                        </td>
                                        <td align="right" colspan="2">
                                            <span t-esc="line.quantity * line.product_id.list_price"/>
                                        </td>
                                    </tr>
                                </t>
                                <tr>
                                    <td align="right" colspan="7">
                                        <strong>Grand Total</strong>
                                    </td>
                                    <td align="right" colspan="1">
                                        <span t-esc="qty"/>
                                    </td>
                                    <td align="right" colspan="2">
                                        <span t-esc="unit_price"/>
                                    </td>
                                    <td align="right" colspan="2">
                                        <span t-esc="total_value"/>
                                    </td>
                                </tr>
                                <tr>
                                    <td align="left" colspan="3">
                                        <span>Amount in Words</span>
                                    </td>
                                    <td colspan="9" align="left">
                                        <span t-esc="o.company_id.currency_id.amount_to_text(int(total_value))"/>
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="8">
                                        <strong>Declaration:-</strong>
                                    </td>
                                    <td colspan="4" rowspan="3">
                                        <strong>M.Kevin John</strong>
                                        <hr style="color:black; border-top:1px solid black; margin-top:1px"/>
                                        <span>Authorized By</span>
                                        <br/>
                                        <span>Date:</span>
                                        <strong t-field="o.scheduled_date"/>
                                        <br/>
                                        <br/>
                                        <br/>
                                        <br/>
                                        <br/>
                                        <strong>Received the above Material in good condition</strong>
                                        <br/>
                                        <br/>
                                        <br/>
                                        <br/>
                                        <br/>
                                        <br/>
                                        <br/>
                                        <br/>
                                        <br/>
                                        <strong>Receiver Signature</strong>
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="8">
                                        <strong>Certified the I /we have checked and found the
                                            quantity and quality of the goods to my /our entire statisfaction and taken
                                            delivery . The amount indicated represent the job charges actually charged
                                            and that there is no flow of additional consideration directly /Indirectly
                                            from the Supplier
                                        </strong>
                                        <strong>Dialog box to be open for entering the comments based on the job . You
                                            can gives us the option to choose based on the supplier.
                                        </strong>
                                        <br/>
                                        <strong>Eg. 1. Material sent for testing on DC is on
                                            returnable basis alongwith Invoice mentioning our DC reference.
                                        </strong>
                                        <br/>
                                        <strong style="margin-left:20px">2. Material sent for rework will be
                                            returned by supplier DC mentioning our DC reference .
                                        </strong>
                                        <br/>
                                        <strong style="margin-left:20px">3.Bin &amp; Covers supplied to the
                                            Vendor on returnable basis.
                                        </strong>
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="8">
                                        <strong>M.Kevin John</strong>
                                        <br/>
                                        <span t-field="o.user_id.company_id.name"/>
                                        <br/>
                                        <t t-if="o.user_id.company_id.street">
                                            <span t-field="o.user_id.company_id.street"/>
                                        </t>
                                        <t t-if="o.user_id.company_id.street2">
                                            <span t-field="o.user_id.company_id.street2"/>,
                                        </t>
                                        <br/>
                                        <t t-if="o.user_id.company_id.city">
                                            <span t-field="o.user_id.company_id.city"/>,
                                        </t>
                                        <t t-if="o.user_id.company_id.state_id.name">
                                            <span t-field="o.user_id.company_id.state_id.name"/>
                                        </t>
                                        <t t-if="o.user_id.company_id.zip">
                                            -
                                            <span t-field="o.user_id.company_id.zip"/>
                                        </t>
                                    </td>
                                </tr>
                            </table>
                            <br/>
                            <br/>
                            <div align="center" class="px-3">
                                <h7 style="color: black; text-align:center; font-weight: bold; letter-spacing: 1px; ">
                                    ******This is computer generated document and requires no signature******
                                </h7>
                            </div>
                        </div>
                    </t>
                </t>
            </t>
        </template>

